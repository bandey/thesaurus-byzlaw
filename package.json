    "i18next-node-fs-backend": "^0.1.3",
    "i18next-xhr-backend": "^1.3.0",
    "keymirror": "^0.1.1",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "object-assign": "^4.1.0",
